#include <iostream>
using namespace std;

int main() {
    system("color a");

    int i, j;
    int a[3][3] = {1, 2, 3, 4, 5, 6, 7, 8, 9};
    int b[3][3];

    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            b[i][j]=a[i][j] * (-1);
            cout<<b[i][j]<< " ";
    }
    cout<<endl;
}
return 0;
}
