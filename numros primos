#include <iostream>
#include <cmath>
#include  <locale.h>

using namespace std;

int main () {

    setlocale(LC_ALL, "Portuguese");
    int n, primo = 1, i = 2;
    cout << "Digite um número de 2 até 100: " << endl;
    cin >> n;


    for ( i; i <n; i++) {
        if (n % i == 0) {
            primo = 0;
        }
    }
    if (primo) {
        cout << "eh primo" << endl;
     }
    else {
        cout << "nao eh primo" << endl;
     }

    return 0;
}
